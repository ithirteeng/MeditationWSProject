<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.meditationwsproject">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MeditationWSProject"
        tools:targetApi="31">
        <activity
            android:name=".ui.main_activity.MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.menu_activity.MenuActivity"
            android:exported="false" />
        <activity
            android:name=".ui.registration_activity.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login_activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.onboarding_activity.OnboardingActivity"
            android:exported="false" />
        <activity
            android:name=".ui.spash_activity.SplashActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>